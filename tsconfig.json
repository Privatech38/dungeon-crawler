{
  "compilerOptions": {
    "outDir": "./dist/",
    "rootDir": "./src/",
    "target": "ES2023",
    "lib": ["ES2023", "DOM"],
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  }
}
