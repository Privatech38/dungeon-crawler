{
  "compilerOptions": {
    "target": "ESNext",
//    "lib": ["ES2023", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  },
  "include": ["src"]
}
